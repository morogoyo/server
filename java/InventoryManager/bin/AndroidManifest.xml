<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inventorymanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.inventorymanager.InventorySelection"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.inventorymanager.NewInventory"
            android:label="@string/title_activity_new_inventory" >
        </activity>
        <activity
            android:name="com.example.inventorymanager.Retire"
            android:label="@string/title_activity_retire" >
        </activity>
        <activity
            android:name="com.example.inventorymanager.Remove"
            android:label="@string/title_activity_remove" >
        </activity>
    </application>

</manifest>
